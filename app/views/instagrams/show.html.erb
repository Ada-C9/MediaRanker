<section>
  <h2>InstagramDetails</h2>
  <h3><%= @instagram.handle %></h3>
  <p><%= @instagram.posts %></p>
  <p><%= @instagram.followers %></p>
  <li>
    <%= link_to "Edit", edit_instagram_path(@instagram) %>
    <%= link_to("Delete", instagram_path(@instagram), method: :delete, data: { confirm: "Are you sure you want to delete this instagram?" }) %>
    <%= link_to "List of instagrams", instagrams_path %>
    <%=link_to "Upvote", instagram_upvote_path(@instagram), method: :post %>
  </li>
</section>

<section>
  <h1>Votes</h1>
  <% @instagram.votes.each do |vote| %>
  <ul>
    <li>
      <%= vote.id %>
    </li>
    <li>User Id:
      <%= vote.user_id%>
    </li>
    <li>Insta Id:
      <%= vote.instagram_id %>
    </li>
  </ul>
  <% end %>

</section>
