<section class=category>
  <ul class=labels>
    <li>Votes</li>
    <li>Title</li>
    <li>Created By</li>
    <li>Published</li>
    <li>Upvote</li>
  </ul>
<% @works.each do |w| %>
  <ul>
    <li>Vote Count TBD</li>
    <li><%= link_to("#{w.title}", work_path(w.id)) %></li>
    <li><%= "#{w.creator}" %></li>
    <% if w.publication_year != nil %>
      <li><%= "#{w.publication_year.year}" %></li>
    <% else %>
      <li>
        <%= "#{w.publication_year}" %>
      </li>
    <% end %>
    <li>
      <%= link_to("Upvote", new_vote_path(w.id)) %>
    </li>
  </ul>
<% end %>
</section>
