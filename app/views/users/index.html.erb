<h3>List of users</h3>
<section>
  <table>
    <th>
      <strong>Username</strong>
    </th>
    <th>
      <strong>Votes</strong>
    </th>
    <th>
      <strong>Joined</strong>
    </th>
    <tr>
      <% @users.each do |user| %>
      <tr>
        <td class="text-center">
          <%= link_to user.name, user_path(user)%>
        </td>
        <td class="text-center">
          <%= user.votes.count %>
        </td>
        <td class="text-center">
          <%=user.created_at.strftime("%B %d,%Y")%>
        </td>

      </tr>
    </tr>
    <%end%>
  </table>
</section>
