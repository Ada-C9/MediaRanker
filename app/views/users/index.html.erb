<h3>List of Users</h3>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.user_name, user_path(user[:id]) %></td>
      <td><%= user.votes.count  %></td>
      <td><%= user.created_at.to_date.to_formatted_s(:long_ordinal) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
