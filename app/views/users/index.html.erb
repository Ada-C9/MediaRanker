<h3>List of Users</h3>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th width="100">Votes</th>
      <th width="200">Joined</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user_path(user.id) %></td>
      <td>8</td>
      <td>Apr  1, 2017</td>
    </tr>
    <% end%>
  </tbody>
</table>

<a class="button" href="/">Back to Media List</a>
