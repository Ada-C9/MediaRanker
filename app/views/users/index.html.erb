<h3>All Users</h3>


      <th>
        Username
      </th>
      <th>
        Votes
      </th>
      <th>
        Joined
      </th>


    <% @users.each do |user| %>
    <tr>
      <td>
        <%= link_to user.name, user_path(user.id) %>
      </td>
      <td>
        <%= user.votes.count %>
      </td>
      <td>
        <%=user.created_at %>
      </td>
    </tr>
    <%end%>
</table>
