<h3>List of Users</h3>
<table>
<thead>
  <tr>
    <th>Username</th>
    <th width="100">Votes</th>
    <th width="100">Joined</th>
  </tr>
</thead>
  <tbody>
    <%@users.each do |user|%>
    <tr>
      <td>
        <%=link_to user.name, user_path(user.id)%>
      </td>
      <td>
        <%=user.votes.count%>
      </td>
      <td>
        <%=user.created_at%>
      </td>
    </tr>
    <%end%>
  </tbody>
</table>
