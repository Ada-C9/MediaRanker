<h3>List of Users</h3>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to(user.username, user_path(user.id)) %></td>
      <td>votecount here</td>
      <td><%= user.join_date %></td>
    </tr>
    <% end %>
  </tbody>
</table>

