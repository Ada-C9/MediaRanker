<h2><%=@user.user_name%></h2>
<section>
  <ul>
    <li><button><%= link_to("Edit", edit_user_path(@user)) %></button></li>
    <li><button><%= link_to("Delete", user_path(@user.id), method: :delete, data: { confirm: "Are you sure you want to delete #{ @user.user_name }?" }) %></li>
    </ul>
  </section>

  <section>
    <ul>
      <li><%=@user.date_joined%></li>
      <li><%=@user.votes%></li>
    </ul>

  </section>
