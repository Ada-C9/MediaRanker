<section>
  <h2>User Details</h2>
  <h3><%= @user.name %></h3>
  <p><%= @user.created_at%></p>
  <li>
    <%= link_to "Edit", edit_user_path(@user) %>
    <%= link_to("Delete", user_path(@user), method: :delete, data: { confirm: "Are you sure you want to delete this user?" }) %>
    <%= link_to "List of users", users_path %>
  </li>
</section>
