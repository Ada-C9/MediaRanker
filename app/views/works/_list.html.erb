<table>
  <tr>
    <th>
      <strong>Votes</strong>
    </th>
    <th>
      <strong>Title</strong>
    </th>
    <th>
      <strong>Created By</strong>
    </th>
    <th>
      <strong>Published</strong>
    </th>
    <th>
      <strong>Upvote</strong>
    </th>
  </tr>

  <% @works.each do |work| %>
  <tr>
    <td>
      <%= work.category %>
    </td>
    <td>
      <%= link_to(work.title, work_path(work)) %>
    </td>
    <td>
      <%= work.creator %>
    </td>
    <td>
      <%= work.publication_year %>
    </td>
    <td>
      Upvote button
    </td>
  </tr>
  <% end %>
</table>
