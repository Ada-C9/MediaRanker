<table>
  <thead>
    <tr>
      <th width="80">
        <strong>Votes</strong>
      </th>
      <th>
        <strong>Title</strong>
      </th>
      <th>
        <strong>Created By</strong>
      </th>
      <th width="100">
        <strong>Published</strong>
      </th>
      <th width="100">
        <strong>Upvote</strong>
      </th>
    </tr>
  </thead>

  <tbody>
    <% works.each do |work| %>
    <tr>
      <td>
        <%=  %>
      </td>
      <td>
        <%= link_to(work.title, work_path(work)) %>
      </td>
      <td>
        <%= work.creator %>
      </td>
      <td>
        <%= work.publication_year %>
      </td>
      <td>
        Upvote
      </td>
    </tr>
    <% end %>
  </tbody>
</table>
