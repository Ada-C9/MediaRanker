<h1>works#index</h1>
<main>
  <table>
    <tr>
      <th>Title</th>
      <th>Created by</th>
      <th>Published</th>
    </tr>
    <% @works.each do |work| %>
    <tr>
      <td><%= link_to(work.title, work_path(work)) %></td>
      <td><%= work.creator%></td>
      <td><%= work.publication_year%></td>
    </tr>
    <% end %>
  </table>
</main>
