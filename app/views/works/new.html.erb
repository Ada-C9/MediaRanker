<h2><%="Add a New Work"%></h2>

<%= form_for @work do |f| %>
<%= f.label :work_type %>
<%= f.text_field :category %>
<%= f.label :name %>
<%= f.text_field :title %>
<%= f.label :creator %>
<%= f.text_field :creator %>
<%= f.label :year_published_produced %>
<%= f.text_field :publication_year %>
<%= f.label :describe_this %>
<%= f.text_field :description %>

<%= f.submit "Add Work" %>
<% end %>
