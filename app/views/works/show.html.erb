<h1 class="page-header"></h1>
<article class="media-details">
  <h3><%= @work.work_title %></h3>
  <p>Created by: <%= @work.work_creator %></p>
  <p>Published: <%= @work.work_publication_year %></p>
  <p><%= @work.work_description %></p>
  <section>
    <a class="button" href="/">Back to media ranks</a>
    <a class="button"> <%= button_to "Edit", edit_work_path %></a>
    <a class="button"><%= button_to "Upvote", edit_work_path %></a>
    <a class="button"><%= button_to "Delete", work_path(@work.id), method: :delete, data: {confirm: "Are you sure?"} %></a>
  </section>
</article>

